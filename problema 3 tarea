// jalir.cpp : Defines the entry point for the console application.
//*Promedio de cinco enteros con dos variables"/

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main()

{

float P=0,A;
cout<<"Introducir el primer numero:"<<endl;
cin>>A;
P=P+A;
cout<<"Introducir el segundo numero:"<<endl;
cin>>A;
P=P+A;
cout<<"Introducir el tercer numero:"<<endl;
cin>>A;
P=P+A;
cout<<"Intoducir el cuarto numero:"<<endl;
cin>>A;
P=P+A;
cout<<"Intoducir el quinto numero:"<<endl;
cin>>A;
P=P+A;
P=P/5;
cout<<"El promedio es:"<<P;


getch ();
}
