// jalir lema.cpp : Defines the entry point for the console application.
//Himno Boivia

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
     cout<<"Bolivianos el hado propicio"<<endl;
	 cout<<"Corono nuestros votos y anhelos"<<endl;
	 cout<<"Es ya libre, ya libre este suelo"<<endl;
	 cout<<"Ya ceso su servil condicion"<<endl;
	 cout<<"Jalir lema"<<endl;
	 getch();
}


^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^



Proceso triangulo
	Escribir "Insertar base:"
	Leer b
	Escribir "Insertar altura:"
	Leer h
	Escribir "Area del triangulo"
	area<-(b*h)/2
	Escribir Area
FinProceso


^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^




Proceso paralelogramo
	Escribir "Insertar base:"
	Leer b
	Escribir "Insertar altura:"
	Leer h
	Escribir "Area del paralelogramo"
	area<- b*h
	Escribir Area
FinProceso

^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^




Proceso circulo
	Escribir "Insertar radio:"
	Leer r
	Escribir "Area del circulo"
	area<-r*r*3.1416
	Escribir Area
FinProceso

^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^


Proceso cuadrado
	Escribir "Insertar lado:"
	Leer l
	Escribir "Area del cuadrado"
	area<-l*l
	Escribir Area
FinProceso

