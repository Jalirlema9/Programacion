// jalir lema.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	cout<<"HolaMundo"<<endl;
	cout<<"Soy -jalir-";
	getch();
}

^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^


Proceso HolaMundo
	Escribir "Hola Mundo"
	Escribir "Soy Jalir"
FinProceso


^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^



Algoritmo HolaMundo
	Escribir 'Bolivianos el hado propicio'
	Escribir 'Corono nuestros votos y anhelos'
	Escribir 'Es ya libre, ya libre este suelo'
	Escribir "Ya ceso su servil condicion"
FinAlgoritmo

